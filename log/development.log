  [1m[36mSQL (49.1ms)[0m  [1mINSERT INTO "entries" ("created_at", "feed_id", "guid", "json", "link", "published_at", "title", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?)[0m  [["created_at", Wed, 02 Oct 2013 14:44:58 UTC +00:00], ["feed_id", 4], ["guid", "http://www.huffingtonpost.com/2013/10/02/tom-clancy-dead_n_4029876.html"], ["json", {:title=>"Tom Clancy Dead: Bestselling Author Dies At 66", :link=>"http://www.huffingtonpost.com/2013/10/02/tom-clancy-dead_n_4029876.html?utm_hp_ref=books&amp;ir=Books", :author=>"madeleine.crum@huffingtonpost.com (Madeleine Crum)", :description=>"Bestselling author Tom Clancy <a href=\"https://twitter.com/publisherswkly/status/385405478077206528\" target=\"_hplink\">died last night</a> in a hospital in Baltimore at the age of 66. <br />\n<br />\nIvan Held, the president of Putnam Books, Clancy's publisher, confirmed his death to the New York Times and<a href=\"https://twitter.com/juliebosman/status/385408452954034176\" target=\"_hplink\"> told them that Clancy \"was a thrill to work with.\"</a><br />\n<br />\nHis latest book, \"Command Authority,\" is slated for publication this December. <br />\n<br />\n<em>This is a developing story...</em>", :pubDate=>2013-10-02 10:31:21 -0400, :guid=>"http://www.huffingtonpost.com/2013/10/02/tom-clancy-dead_n_4029876.html"}], ["link", "http://www.huffingtonpost.com/2013/10/02/tom-clancy-dead_n_4029876.html?utm_hp_ref=books&amp;ir=Books"], ["published_at", Wed, 02 Oct 2013 14:31:21 UTC +00:00], ["title", "Tom Clancy Dead: Bestselling Author Dies At 66"], ["updated_at", Wed, 02 Oct 2013 14:44:58 UTC +00:00]]
